{
  "name": "course-management-platform",
  "version": "1.0.0",
  "description": "Course management platform for academic institutions",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests --testPathIgnorePatterns=tests/unit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:module2": "node scripts/test-module2.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "worker": "node workers/notificationWorker.js",
    "test:db": "node scripts/test-db-connection.js",
    "setup:sqlite": "node scripts/setup-sqlite.js"
  },
  "keywords": [
    "course-management",
    "nodejs",
    "express",
    "mysql"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "i18next": "^23.7.6",
    "i18next-fs-backend": "^2.2.1",
    "i18next-http-middleware": "^3.5.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.5",
    "nodemailer": "^6.9.7",
    "redis": "^4.6.10",
    "sequelize": "^6.35.1",
    "sqlite3": "^5.1.7",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3"
  }
}
